@page
@model MadStickWebAppTester.Pages.Endpoints.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Endpoint</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            The value: @Model.Endpoint.Value
        </dt>
        <dd class="col-sm-10">
            Allowed to access: @Model.Endpoint.IsAllowedAccess
        </dd>
        <dd class="col-sm-10">
            Allowed to modify: @Model.Endpoint.IsAllowedModification
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Endpoint?.EndpointId">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
