@using MadStickWebAppTester.Data.UserEntity
@model ApplicationUser
@{

    <a class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage">
        Hello
        @User.Identity.Name
    </a>



    <a class="nav-link text-dark" asp-page="/Orders/Details" method="post" asp-route-userId="">
        Orders
    </a>


    <form class="form-inline" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Page("/", new { area = "" })"
    asp-area="Identity" method="post">
        <button class="nav-link btn btn-link text-dark" type="submit">
            Logout
        </button>
    </form>

}
