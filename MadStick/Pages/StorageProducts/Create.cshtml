@page
@model MadStickWebAppTester.Pages.StorageProducts.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>StorageProduct</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div class="form-group">
                <label asp-for="StorageProduct.MadStickProductId" class="control-label"></label>
                <select asp-for="StorageProduct.MadStickProductId" class="form-control" asp-items="@Model.ViewLists.Products"></select>
                <span asp-validation-for="StorageProduct.MadStickProductId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="StorageProduct.StorageUnitId" class="control-label"></label>
                <select asp-for="StorageProduct.StorageUnitId" class="form-control" asp-items="@Model.ViewLists.StorageUnits"></select>
                <span asp-validation-for="StorageProduct.StorageUnitId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="StorageProduct.AmountLeft" class="control-label"></label>
                <input asp-for="StorageProduct.AmountLeft" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Back to List</a>
</div>

